<mvvm:PrismApplication x:Class="MoocDownloader.App"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:mvvm="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/HandyControl;component/Themes/SkinDefault.xaml" />
                <ResourceDictionary Source="/HandyControl;component/Themes/Theme.xaml" />

                <ResourceDictionary Source="Styles/GlobalStyle.xaml" />
                <ResourceDictionary Source="Resources/BrushResource.xaml" />
                <ResourceDictionary Source="Resources/ConverterResource.xaml" />
                <ResourceDictionary Source="Resources/CredentialResource.xaml" />
                <ResourceDictionary Source="Languages/zh-CN.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</mvvm:PrismApplication>